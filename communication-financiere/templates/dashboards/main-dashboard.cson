    source: "Bilan 2017"
    id: 1
    badge: "evolution_down"
    comment: "%%Compared to 8% growth in 2016%%"
    badgeSentiment: "worse"
  ,
    name: "%%Financial Communication%%"
    type: "pdf"
    data:
      query:
        domain: "pdf_home"
