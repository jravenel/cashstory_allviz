entityName: "{{ dashboards }}"
entityGroup: "Region"
id: "{{ id }}"
report: "{{ report_id }}"
default: "true"

categories: [
  name: "%%Group Total%%"
  kpis: [
    name: "%%Revenue%%"
    value: "{{ kpi1 }}"
    unit: "%%Billion €%%"
    slide: 10001
    source: "Bilan 2017"
    badge: "evolution_up"
    badgeSentiment: "better"
    comment: "%%Revenue of 29 Billions in 2016%%"
    id: 0
  ,
    type: "media-img"
